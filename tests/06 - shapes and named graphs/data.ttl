@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh:   <http://www.w3.org/ns/shacl#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix ex:  <http://example.org/> .
@prefix tree: <https://w3id.org/tree#> . 


ex:C a tree:Collection ;
    tree:shape ex:Shape ;
    tree:member ex:M1v1, ex:M1v2, ex:M2v1 .

ex:M1v1 ex:subject ex:M1 .
ex:M1v2 ex:subject ex:M1 .

ex:M1v1 {
    ex:M1 rdfs:label "M1v1" .
}

ex:M1v2 {
    ex:M1 rdfs:label "M1v2" .
}

# M2v2 uses named graphs for a different purpose than member extraction
<https://example.org/originallyFromSource1> {
    ex:M2v1 a ex:TestThing1 .  
}
<https://example.org/originallyFromSource2> {
    ex:M2v1 a ex:TestThing2 .
}
     
ex:M2v1 {
    ex:M2v1 a ex:TestThing3 .
}